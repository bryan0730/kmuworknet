<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kmu.worknet.job.mappers.testMapper">

	<select id="findByMovieList" resultType="com.kmu.worknet.job.domain.testDomain">
		SELECT *
		FROM ratings
		WHERE memSeq = #{memSeq}
		;
	</select>
	<select id="findBySelectOne" resultType="com.kmu.worknet.job.domain.testDomain">
		SELECT *
		FROM movies
		WHERE movieId = #{movieId}
		;
	</select>



</mapper>